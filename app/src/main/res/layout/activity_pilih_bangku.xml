            android:textColor="@color/putih"
            android:textSize="25sp"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="5dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@drawable/button_line_biru_muda" />

